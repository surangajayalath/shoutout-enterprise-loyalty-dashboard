  "scripts": {
    "start": "set PORT=8080 && set \"REACT_APP_BUILD_VARIANT=dev\" && npm i && react-scripts start",
    "start:dilmah": "set PORT=8080 && set \"REACT_APP_BUILD_VARIANT=dilmah\" && react-scripts start",
    "build:staging": "set \"REACT_APP_BUILD_VARIANT=dev\" && react-scripts build",
    "build": "react-scripts --max_old_space_size=8192 build",
    "build:staging:github": "react-scripts --max_old_space_size=8192 build",
    "build:dilmah": "set \"REACT_APP_BUILD_VARIANT=dilmah\" && react-scripts --max_old_space_size=8192 build",
    "test": "react-scripts test --verbose false --coverage",
    "eject": "react-scripts eject",
    "deploy:staging": "firebase deploy --only hosting:staging",
    "deploy:dev": "firebase deploy --only hosting:dev",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^7.31.2",
    "husky": "^7.0.0",
    "jira-prepare-commit-msg": "^1.6.2",
